#linked list cycle
#difficulty: easy
#language: python
#link:https://leetcode.com/problems/linked-list-cycle/
class Solution:
    def hasCycle(self, head: Optional[ListNode]) -> bool:
        l=[]
        cur=head
        while cur:
            if cur in l:
                return True
            l.append(cur)
            cur=cur.next
        return False
